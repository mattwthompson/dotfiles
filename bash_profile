source ~/.bashrc

# The git-completion.bash script was not put in the right place by default; 
# See for details: https://www.linkedin.com/pulse/how-enable-git-tab-completion-bash-mac-os-x-conor-livingston/
source /usr/local/etc/bash_completion.d/git-completion.bash

# added by Anaconda 2.3.0 installer
export PATH="//anaconda/bin:$PATH"
